@model SistemaFinanceiroUGB232.Models.Departamento

@{
    ViewData["Title"] = "Detalhes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>

    <div>
        <h4>Listagem</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.DepartamentoID)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DepartamentoID)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Nome)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Nome)
            </dd>
        </dl>
    </div>
    <div>
        @Html.ActionLink("Editar", "Edit", new { id = Model.DepartamentoID }) |
        <a asp-action="Index">Voltar</a>
    </div>
</body>
</html>
